{
    "name": "zivvio/zivvio",
    "description": "The Zivvio Framework",
    "keywords": [
        "zivvio",
        "framework"
    ],
    "homepage": "https://github.com/zivvio/zivvio",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Yii Team",
            "email": "team@yiiframework.com",
            "homepage": "https://www.yiiframework.com/team",
            "role": "Founder and project lead"
        }
    ],
    "support": {
        "issues": "https://github.com/zivvio/zivvio/issues?state=open",
        "source": "https://github.com/zivvio/zivvio"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2.0",
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "lib-pcre": "*",
        "zivvio/composer": "~5.0.0",
        "ezyang/htmlpurifier": "^4.17",
        "cebe/markdown": "~1.0.0 | ~1.1.0 | ~1.2.0",
        "bower-asset/jquery": "3.7.*@stable | 3.6.*@stable | 3.5.*@stable | 3.4.*@stable | 3.3.*@stable | 3.2.*@stable | 3.1.*@stable | 2.2.*@stable | 2.1.*@stable | 1.11.*@stable | 1.12.*@stable",
        "bower-asset/inputmask": "^5.0.8",
        "bower-asset/punycode": "^2.2",
        "bower-asset/yii2-pjax": "~2.0.1"
    },
    "require-dev": {
        "cebe/indent": "~1.0.2 | ~1.1.0",
        "dealerdirect/phpcodesniffer-composer-installer": "*",
        "dms/phpunit-arraysubset-asserts": "^0.5",
        "phpunit/phpunit": "^9.6"
    },
    "scripts": {
        "iii": [
            "rm -f composer.lock",
            "@composer install"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "yii\\": "framework/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yiiunit\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "zivvio/composer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "bin": [
        "framework/yii"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        }
    }
}
